#include <iostream>

int main() {
    int a = 1;
    int b = 2;
    int c = 3;
    int d = 4;
    std::cout << "Initial values:" << std::endl;
    std::cout << "a = " << a << ", b = " << b << ", c = " << c << ", d = " << d << std::endl;
    int t = a;
    a = b;
    b = c;
    c = d;
    d = t;
    std::cout << "Values after rearrangement:" << std::endl;
    std::cout << "a = " << a << ", b = " << b << ", c = " << c << ", d = " << d << std::endl;

    return 0;
}
